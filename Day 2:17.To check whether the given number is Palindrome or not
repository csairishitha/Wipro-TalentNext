public class Demo
{
    public static void main(String args[])
    {
        int n=1232;
        int dig,sum=0;
        int temp=n;
        while(temp>0)
        {
        dig=temp%10;
        sum=sum*10+dig;
        temp=temp/10;
        }
        if(n==sum)
        {
        System.out.println("palindrome");
        }
        else
        {
            System.out.println("not palindrome");
        }
    }
}
