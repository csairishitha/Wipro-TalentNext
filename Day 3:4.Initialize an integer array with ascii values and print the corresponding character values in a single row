class Demo
{
    public static void main(String args[])
    {
        int[] arr=new int[10];
        Scanner s=new Scanner(System.in);
        System.out.println("enter the elements in array");
        for(int i=0;i<5;i++)
        {
            arr[i]=s.nextInt();
        }
    for(int i=0;i<5;i++)
    {
        System.out.println((char)arr[i]+" ");
    }
}
}
