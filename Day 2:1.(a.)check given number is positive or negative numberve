import java.util.*;
public class program
{
    public static void main(String args[])
    {
        int x=7;
        if(x<0)
        {
            System.out.println("negative number");
        }
        else if(x>0)
        {
            System.out.println("positive number");
        }
        else
        {
            System.out.println("zero");
        }
    }
}
