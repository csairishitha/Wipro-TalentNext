class Demo
{
    public static void main(String[] args) {
        int[] arr1=new int[5];
        System.out.println("enter the elements in array:");
        Scanner in=new Scanner(System.in);
        for(int i=0;i<5;i++)
        {
            arr1[i]=in.nextInt();
            
        }
        Arrays.sort(arr1);
        System.out.println("the sorted order is:");
        for(int i=0;i<5;i++)
        {
        System.out.println(arr1[i]);
        }
    }
}
