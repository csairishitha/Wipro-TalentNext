class Demo
{
    public static void main(String args[])
    {
        int[][] arr={{1,23,45},{55,121,222},{56,77,89}};
        if(arr.length!=0||arr[0].length!=3|| arr[1].length!=3|| arr[2].length!=3)
        {
            System.out.println("please enter all elements");
        }
        int lar=arr[0][0];
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                if(arr[i][j]>lar)
                {
                    lar=arr[i][j];
                }
            }
        }
        System.out.println("biggest element is:"+lar);
    } 
}

